{
  "name": "threekit-poc",
  "version": "0.1.0",
  "packageManager": "yarn@4.1.0",
  "engines": {
    "node": ">20.0"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "styles": "./dist/index.css"
    },
    "./css": "./dist/style.css",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.umd.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "start": "treble-scripts start",
    "build": "treble-scripts build",
    "build:vite": "vite build",
    "format": "prettier --write src",
    "lint": "eslint src",
    "serve": "node server"
  },
  "dependencies": {
    "@storefront-ui/react": "^2.6.3",
    "@storefront-ui/shared": "^2.5.1",
    "@threekit-tools/treble": "0.0.93",
    "@threekit-tools/treble-scripts": "0.0.44",
    "classnames": "^2.5.1",
    "cors": "2.8.5",
    "express": "4.19.2",
    "morgan": "1.10.0",
    "postcss": "^8.4.39",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.77.6",
    "tailwindcss": "3.4.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "rimraf": "^6.0.0",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.2.1"
  }
}
